<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ml.ernestovector.mascotas">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true">
        <activity
            android:name=".actividades.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".actividades.favoritos"
            android:label="Favoritos"
            android:theme="@style/ToolBarTheme"
            android:parentActivityName=".actividades.MainActivity">

        </activity>
        <activity
            android:name=".actividades.Contacto"
            android:label="@string/mContacto"
            android:theme="@style/ToolBarTheme"
            android:parentActivityName=".actividades.MainActivity">

        </activity>
        <activity
            android:name=".actividades.AcercaDe"
            android:label="@string/mAcercaDe"
            android:theme="@style/ToolBarTheme"
            android:parentActivityName=".actividades.MainActivity">

        </activity>
    </application>

</manifest>